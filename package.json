{
  "name": "gonzalez-mobile-monorepo",
  "private": true,
  "workspaces": [
    "apps/*"
  ],
  "devDependencies": {
    "turbo": "^2.5.6"
  },
  "scripts": {
    "dev:mobile": "cd apps/mobile && expo start",
    "dev:server": "cd apps/server && next dev",
    "dev": "turbo run dev",
    "build": "cd apps/server && npm run build",
    "start": "cd apps/server && npm start",
    "postinstall": "cd apps/server && npm install",
    "lint": "turbo run lint"
  }
}
